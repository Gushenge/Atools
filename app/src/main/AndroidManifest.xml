<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.gushenge.atools.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".activity.App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:networkSecurityConfig="@xml/network"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.util.KeyboardActivity" />
        <activity android:name=".activity.ui.RollingTextViewActivity" />
        <activity android:name=".activity.util.ALogActivity" />
        <activity android:name=".activity.util.ASystemActivity" />
        <activity android:name=".activity.util.APreferenceActivity" />
        <activity android:name=".activity.util.AViewActivity" />
        <activity android:name=".activity.util.ARandomActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".activity.ui.ArcButtonActivity" />
        <activity android:name=".activity.util.ADateActivity" />
        <activity android:name=".activity.UtilsActivity" />
        <activity android:name=".activity.UiActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ui.AutoHeightImageActivity" />
    </application>

</manifest>